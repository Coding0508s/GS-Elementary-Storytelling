[Unit]
Description=Laravel Queue Worker for Storytelling
After=network.target

[Service]
Type=simple
User=andrewhur
WorkingDirectory=/Applications/XAMPP/xamppfiles/htdocs/storytelling
ExecStart=/usr/bin/php artisan queue:work --sleep=3 --tries=3 --max-time=3600
Restart=always
RestartSec=5
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
